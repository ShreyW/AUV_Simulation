<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <param name="robot_description" command="cat '$(find diff_drive_description)/urdf/diff_drive.urdf'" />

    <arg name="x" default="1"/>
    <arg name="y" default="0"/>
    <arg name="z" default="0"/>

    <node name="RoboBot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
          args="-urdf -param robot_description -model diff_drive -x $(arg x) -y $(arg y) -z $(arg z)" />

</launch>
